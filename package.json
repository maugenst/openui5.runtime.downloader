  "dependencies": {
    "adm-zip": "^0.4.7",
    "fs-extra": "^3.0.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.7.1",
    "prettysize": "^0.1.0",
    "readdirp": "^2.1.0",
    "request": "^2.81.0",
    "request-progress": "^3.0.0"
  }
