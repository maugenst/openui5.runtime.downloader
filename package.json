{
  "name": "openui5.runtime.downloader",
  "version": "2.0.6",
  "description": "This bundle downloads the officially released OpenUI5 runtime zip from http://openui5.org/download.html, unpacks them locally and exposes them in the node_modules directory to be consumable by a static webserver.",
  "main": "index.js",
  "scripts": {
    "postinstall": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maugenst/openui5.runtime.downloader.git"
  },
  "keywords": [
    "openui5",
    "npm",
    "packager"
  ],
  "author": "Marius Augenstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maugenst/openui5.runtime.downloader/issues"
  },
  "homepage": "https://github.com/maugenst/openui5.runtime.downloader#readme",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "fs-extra": "^3.0.1",
    "is-reachable": "^2.3.3",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "node-fetch": "^1.7.1",
    "prettysize": "^0.1.0",
    "readdirp": "^2.1.0",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "request-promise-native": "^1.0.5"
  },
  "openui5": {
    "version": "latest",
    "stable": "true",
    "downloadHost": "openui5.hana.ondemand.com"
  }
}
